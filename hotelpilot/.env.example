# HotelPilot Environment Configuration
# Copy this to .env and add your actual API keys

# Google AI (Gemini) API Key - Get from https://aistudio.google.com/app/apikey
GOOGLE_AI_API_KEY=your_gemini_api_key_here

# Environment
ENVIRONMENT=development
DEBUG=true

# Models
PRIMARY_MODEL=gemini-2.5-flash
FAST_MODEL=gemini-2.5-flash

# Retell AI Configuration (for phone bookings)
# Sign up at https://retell.ai to get your API key
RETELL_API_KEY=your_retell_api_key_here
RETELL_AGENT_ID=  # Leave empty to auto-create agent

# Optional: Google Cloud (for better voice quality)
# GOOGLE_CLOUD_CREDENTIALS=/path/to/credentials.json
